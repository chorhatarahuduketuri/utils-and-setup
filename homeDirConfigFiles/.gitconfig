[user]
    name = 
    email = 
[color]
    diff = auto
    status = auto
    branch = auto
[merge]
    tool = vimdiff
[alias]
    co = checkout
    #tree = log --graph --pretty=oneline --abbrev-commit --all --decorate
    tree = log --graph --pretty=format:'%C(auto) %h %ad [%an] %m%m %d %s' --abbrev-commit --all --decorate --date=short
    br = branch
    dif = diff --stat
    s = status
    re = rebase -i
    fe = fetch --all -p
    fare = !git fetch --all -p && git rebase -i
[rebase]
    autosquash = true
[rerere]
    enabled = 0
[core]
    excludesfile = ~/.gitignore_global
    editor = vim
[credential]
    helper = cache
